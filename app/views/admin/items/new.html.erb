<h1>商品新規登録</h1>
　<%= form_with model: @item, local:true do |f| %>
  <h3>商品画像</h3>
  <%= f.attachment_field :image %>
  <h3>商品名</h3>
  <%= f.text_field :item_name %>
  <h3>商品説明</h3>
  <%= f.text_area :introduction %>
  <h3>ジャンル名</h3>
  <!--ジャンル検索-->
  <h3>税抜価格</h3>
  <%= f.text_field :price %>
  <h3>販売ステータス</h3>
  <%= f.radio_button :is_status, true %><p>販売中</p>
  <%= f.radio_button :is_status, false %><p>販売停止</p>
  <%= f.submit '新規登録' %>
<% end %>

<div class="done">
